{
  "extends": "expo-module-scripts/tsconfig.base",
  "compilerOptions": {
    "outDir": "./build",
    "paths": {
      "@/*": ["./src/*"]
    },
    "strictPropertyInitialization": false,
    "strictNullChecks": false
  },
  "include": ["./src"],
  "exclude": ["**/__mocks__/*", "**/__tests__/*", "**/__rsc_tests__/*"]
}
